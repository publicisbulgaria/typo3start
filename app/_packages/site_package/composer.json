{
    "name":"pubbg/site-package",
    "type":"typo3-cms-extension",
    "description":"Example site package",
    "license":"MIT",
    "keywords":["typo3", "site package"],
    "version": "1.0.1",
    "authors":[
        {
            "name":"",
            "role":"Developer",
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0 || ^8.1 || ^8.2",
        "typo3/cms-core":"^11.5",
        "typo3/cms-fluid-styled-content":"^11.5"
    },
    "require-dev": {
        "typo3/testing-framework": "^7.0"
    },
    "autoload":{
        "psr-4":{
            "PUBBG\\SitePackage\\":"Classes/"
        }
    },
    "autoload-dev":{
        "psr-4":{
            "PUBBG\\SitePackage\\Tests\\":"Tests/"
        }
    },
    "extra":{
        "typo3/cms":{
            "extension-key":"site_package"
        }
    },
    "config": {
        "allow-plugins": true,
        "vendor-dir": ".composer/vendor",
        "bin-dir": ".composer/bin"
    }
}